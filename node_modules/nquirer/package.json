{
  "name": "nquirer",
  "version": "0.1.8",
  "description": "Hierarchical configuration with nconf combined with inquirer to prompt user for missing configuration.",
  "main": "dist/nquirer.js",
  "scripts": {
    "jsdoc": "./node_modules/jsdoc/jsdoc.js src -d docs",
    "test": "./node_modules/mocha/bin/mocha --compilers js:babel-register test/**/*.spec.js",
    "dist": "./node_modules/babel-cli/bin/babel.js src -d dist",
    "build": "npm run jsdoc && npm run dist"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:coreyferguson/nquirer.git"
  },
  "keywords": [
    "nquirer",
    "nconf",
    "inquirer",
    "configuration",
    "config",
    "prompt"
  ],
  "author": "Corey Ferguson <corey.t.ferguson@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coreyferguson/nquirer/issues"
  },
  "homepage": "https://github.com/coreyferguson/nquirer#readme",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2016": "^6.16.0",
    "babel-register": "^6.16.3",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "deep-freeze": "0.0.1",
    "jsdoc": "^3.4.2",
    "mocha": "^3.1.2",
    "sinon": "^1.17.6"
  },
  "dependencies": {
    "inquirer": "^1.2.2",
    "nconf": "^0.8.4"
  }
}
